        <loc>@{appConfig.protocol}@{appConfig.allowedHost}/sitemap/sitemap-events.xml</loc>
        <changefreq>weekly</changefreq>
        <priority>0.8</priority>
    </url>
    <url>
        <loc>@{appConfig.protocol}@{appConfig.allowedHost}/sitemap/sitemap-surnames.xml</loc>
        <changefreq>weekly</changefreq>
        <priority>0.8</priority>
    </url>
    <url>
        <loc>@{appConfig.protocol}@{appConfig.allowedHost}/sitemap/sitemap-firstnames.xml</loc>
        <changefreq>weekly</changefreq>
        <priority>0.8</priority>
    </url>
</urlset>